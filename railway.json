{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "nixpacks": {
            "phases": {
                "install": {
                    "pkgs": ["python39", "pip"]
                }
            }
        }
    },
    "deploy": {
        "startCommand": "python3 manage.py migrate && gunicorn ReviewDosen.wsgi",
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 10
    }
}
